{
"manifest_version": 3,
"name": "PhishGuard",
"version": "1.0",
"description": "Detects phishing websites using a FastAPI backend.",
"permissions": ["activeTab", "scripting"],
"host_permissions": ["http://127.0.0.1:8000/"],
"action": {
"default_popup": "popup.html",
"default_icon": "icon.png"
},
"content_scripts": [
{
"matches": ["<all_urls>"],
"js": ["popup.js"]
}
]
}